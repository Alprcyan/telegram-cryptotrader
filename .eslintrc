{
    "extends": [
        "eslint:recommended",
        "google"
    ],
    "env": {
        "node": true,
        "es6": true
    },
    "parserOptions": {
        "ecmaVersion": 6
    },
    "rules": {
        "linebreak-style": "off",
        "no-console": "off",
        "quotes": ["error", "double"],
        "max-len": ["error", 120],
        "one-var": "off"
    }
}
